package buildings;

/**
 *
 * @author 17brennerm
 */

public class Apartment
{
    private int floor, room,ppl;
    private double rent;
    private boolean air;
    public Apartment()
    {
        floor =3;
        room= 2;
        ppl =10;
        rent= 11;
        air= true;
        
    }
public Apartment(int f, int r, int p, int re, boolean ac)
{
   setfloor(f);
setroom(r);
setppl(p);
setrent(re);
 setair(ac);
}
    


public int setfloor(int f)
{
        return floor;
}
public String setroom(int r)
{
    if (room>1 || room<10){
        return ;
    }else{
        return "Exceeds Limit";
    }
    }
public int setppl(int p)
{
        return ppl;
    }
public double setrent(int re)
{
        return rent;
}
public boolean setair(boolean ac)
{
    return air;
}
    
    
    
    
    
    
    
}
