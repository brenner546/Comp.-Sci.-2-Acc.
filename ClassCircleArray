package circlesarray;

/**
 *
 * @author 17brennerm
 */


public class Circles extends Object 
{
   
    private int radius;
    private char character;
   
public Circles(int r, char c)
{
  setradius(r);
  setcharacter(c);
}

public int getradius()
{
    return radius;
}

public int setradius(int rad)
{
  radius = rad;
  return rad;
}
public char getcharacter()
{
    return character;
}

public char setcharacter(char clist)
{
 character =clist;
  return clist;
}



}
